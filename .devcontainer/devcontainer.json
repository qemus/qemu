{
  "name": "Linux Mint",
  "service": "qemu",
  "containerEnv": {
    "BOOT": "mint"
  },  
  "forwardPorts": [8006],  
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "openBrowser"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "silent"
  }, 
  "dockerComposeFile": "codespaces.yml"
}
